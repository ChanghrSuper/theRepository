<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.chr.dao.MasterSlaveDao">
  
  <insert id="insert" parameterType="masterSlave">
  	insert into master_slave values(#{id},#{name});
  </insert>
  
  <select id="selectAll" resultType="masterSlave">
  	select id,name from master_slave;
  </select>
</mapper>
